{
	"name": "edge",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write .",
		"codegen": "npx @edgedb/generate edgeql-js && npx @edgedb/generate interfaces",
		"migrate": "edgedb migration create && edgedb migration apply && pnpm codegen"
	},
	"devDependencies": {
		"@edgedb/generate": "^0.3.3",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.25.1",
		"@types/bcrypt": "^5.0.0",
		"@types/jsonwebtoken": "^9.0.3",
		"@unocss/reset": "^0.55.7",
		"@unocss/transformer-directives": "^0.55.7",
		"@unocss/transformer-variant-group": "^0.55.7",
		"@vercel/blob": "^0.13.0",
		"bcrypt": "^5.1.1",
		"edgedb": "^1.3.6",
		"jsonwebtoken": "^9.0.2",
		"lucide-svelte": "^0.269.0",
		"malachite-ui": "^0.9.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"unocss": "^0.55.7",
		"valibot": "^0.13.1",
		"vite": "^4.4.9"
	},
	"type": "module",
	"dependencies": {
		"vercel": "^32.4.1"
	}
}
